# 数据库连接
DATABASE_URL=postgresql://username:password@localhost:5432/idaas_dev

# JWT密钥（生产环境请使用强密钥）
# 生成64字节随机密钥（推荐）
# openssl rand -base64 64
# 生成32字节随机密钥（最小要求）
# openssl rand -base64 32
# 生成十六进制密钥
# openssl rand -hex 64
# JWT算法（可选HS256或RS256，默认HS256）
JWT_ALGORITHM=HS256

# --- HS256配置（对称密钥）---
# 用户JWT密钥（强烈建议32字节以上，生产环境必须≥32字符，否则无法启动）
JWT_USER_SECRET_KEY=your-user-secret-key
# 服务JWT密钥（强烈建议32字节以上，生产环境必须≥32字符，否则无法启动）
JWT_SERVICE_SECRET_KEY=your-service-secret-key

# --- RS256配置（非对称密钥，PEM内容或文件路径）---
# 用户JWT私钥（PEM内容或文件路径）
# JWT_USER_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----..."
# JWT_USER_PRIVATE_KEY=./rsa_user_private.pem
# 用户JWT公钥（PEM内容或文件路径）
# JWT_USER_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----..."
# JWT_USER_PUBLIC_KEY=./rsa_user_public.pem
# 服务JWT私钥（PEM内容或文件路径）
# JWT_SERVICE_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----..."
# JWT_SERVICE_PRIVATE_KEY=./rsa_service_private.pem
# 服务JWT公钥（PEM内容或文件路径）
# JWT_SERVICE_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----..."
# JWT_SERVICE_PUBLIC_KEY=./rsa_service_public.pem

# --- ES256配置（非对称密钥，PEM内容或文件路径，曲线为prime256v1）---
# 用户JWT私钥（PEM内容或文件路径）
# JWT_USER_PRIVATE_KEY="-----BEGIN EC PRIVATE KEY-----..."
# JWT_USER_PRIVATE_KEY=./es256_user_private.pem
# 用户JWT公钥（PEM内容或文件路径）
# JWT_USER_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----..."
# JWT_USER_PUBLIC_KEY=./es256_user_public.pem
# 服务JWT私钥（PEM内容或文件路径）
# JWT_SERVICE_PRIVATE_KEY="-----BEGIN EC PRIVATE KEY-----..."
# JWT_SERVICE_PRIVATE_KEY=./es256_service_private.pem
# 服务JWT公钥（PEM内容或文件路径）
# JWT_SERVICE_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----..."
# JWT_SERVICE_PUBLIC_KEY=./es256_service_public.pem

# 用户Token有效期（单位：秒，默认3600=1小时）
USER_TOKEN_EXPIRATION=3600

# 服务Token有效期（单位：秒，默认300=5分钟）
SERVICE_TOKEN_EXPIRATION=300

# 端口
PORT=8080
# 运行环境
GO_ENV=development 